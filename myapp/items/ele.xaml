<UserControl x:Class="myapp.items.ele"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:local="clr-namespace:myapp.items"
             mc:Ignorable="d"
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid>
        <TextBox Visibility="Hidden" x:Name="mark" Height="21.167" Margin="1,29,0,-0.167" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="10" Foreground="#FF0C0C0C"/>
        <Ellipse x:Name="el1" Margin="7,2,7,12" Fill="Black" />
        <Ellipse x:Name="el2" Margin="10,5,10,15" Fill="White" />
        <Ellipse x:Name="el3" Margin="15.333,10.333,15.333,20.333" Fill="White" />
    </Grid>
</UserControl>

